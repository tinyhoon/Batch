1:SELECT EQUIP_MGMT_NUM,TPO_CD,EQUIP_CD,EQUIP_SYS_NM,EQUIP_MGMT_DEPT_ORG_ID,EQUIP_TYP_CL_CD,COMM_CD_VAL_DESC,LINE_NUM,EQUIP_TID_VAL,EMS_IP_ADDR,MGMT_TPO_CD,EQUIP_SET_LOC_DESC,LINK_EQUIP_MGMT_NUM,MEMO FROM (SELECT E.EQUIP_MGMT_NUM,E.TPO_CD,EQUIP_CD,E.EQUIP_SYS_NM,E.EQUIP_MGMT_DEPT_ORG_ID,E.EQUIP_TYP_CL_CD,C.COMM_CD_VAL_DESC,E.LINE_NUM,E.EQUIP_TID_VAL,E.EMS_IP_ADDR,E.MGMT_TPO_CD,E.EQUIP_SET_LOC_DESC,E.LINK_EQUIP_MGMT_NUM,E.MEMO FROM UKEY.ZOSS_EQUIP_SYS E INNER JOIN (SELECT * FROM UKEY.ZNGM_COMM_CD_DTL WHERE ROWNUM <= 100 AND COMM_CD_ID = 'ZOSS_C_EQTYP') C ON EQUIP_TYP_CL_CD = C.COMM_CD_VAL WHERE ROWNUM <= 10) A
2:SELECT TPO_CD,MGMT_TPO_CD,TPO_NM,TPO_TYP_CD,COMM_CD_VAL_NM FROM (SELECT T.TPO_CD,T.MGMT_TPO_CD,T.TPO_NM,T.TPO_TYP_CD,C.COMM_CD_VAL_NM FROM UKEY.ZOSS_TPO T INNER JOIN (SELECT * FROM UKEY.ZNGM_COMM_CD_DTL WHERE ROWNUM <= 10 AND COMM_CD_ID = 'ZOSS_C_F0043') C ON T.TPO_TYP_CD = C.COMM_CD_VAL WHERE ROWNUM <= 10) A
